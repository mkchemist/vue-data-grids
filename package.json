{
  "name": "vue-data-grids",
  "version": "1.0.2",
  "description": "Vue.js data-tables",
  "main": "dist/vue-data-grids.ssr.js",
  "browser": "dist/vue-data-grids.esm.js",
  "module": "dist/vue-data-grids.esm.js",
  "unpkg": "dist/vue-data-grids.min.js",
  "keywords": [
    "data-tables", "datatables", "dataTables","vue-data-tables",
    "data-grids", "vue-data-grids"
  ],
  "author": {
    "email": "m.k_chemist@yahoo.com",
    "name": "Mohamed Kamal",
    "url": "https://github.com/mkchemist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mkchemist/vue-data-grids.git"
  },
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "sideEffects": false,
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.js",
    "prebuild": "rimraf ./dist",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "bootstrap": "4.6",
    "cross-env": "^7.0.3",
    "lodash": "^4.17.21",
    "minimist": "^1.2.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.52.8",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^5.1.9",
    "vue": "^2.6.14",
    "vue-template-compiler": "^2.6.14"
  },
  "peerDependencies": {
    "vue": "^2.6.14"
  },
  "engines": {
    "node": ">=12"
  }
}
